# Gmail SMTP config
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        /var/log/msmtp.log

account gmail
host smtp.gmail.com
port 587
from hristo.rusev92@gmail.com
user hristo.rusev92@gmail.com
passwordeval "echo $GMAIL_APP_PASSWORD"

account default : gmail
